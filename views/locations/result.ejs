<div class="center-align">
  <h1>Results</h1>
</div>
<% locations.forEach(location => { %>
<div class="container green lighten-3 center-align">
  <h4><%= location.name %></h4>
  <div class="left-align">
    <h5>Region: <%= location.region %></h5>
    <h5>Country: <%= location.country %></h5>
    <h5>latitude: <%= location.lat %></h5>
    <h5>longitude: <%= location.lon %></h5>
  </div>
  <form method="GET" action="locations/detail">
    <input type="hidden" name="lat" id="lat" value="<%= location.lat %>" />
    <input type="hidden" name="lon" id="lon" value="<%= location.lon %>" />
    <div class="right-align">
      <button class="btn waves-effect waves-light green" type="submit">
        <i class="material-icons right">find_in_page</i>Details
      </button>
    </div>
  </form>
</div>

<% }) %>
